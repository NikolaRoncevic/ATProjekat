package models;

import java.io.Serializable;

import javax.ejb.Stateful;

@Stateful
public class Agent implements Serializable {
	
	private AID id;
	
	public void handleMessage(ACLMessage message) {
		
	}
	public AID getId() {
		return id;
	}
	public void setId(AID id) {
		this.id = id;
	}
	public Agent(AID id) {
		super();
		this.id = id;
	}
	public Agent() {
		
	}
	
	
	

}
